{
    "name": "oromedialab/nxcorebundle",
    "license": "proprietary",
    "private": true,
    "version": "0.0.1",
    "type": "symfony-bundle",
    "description": "Nx Core Bundle",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ibrahim Azhar Armar",
            "email": "ibrahim.azhar@oromedialab.com",
            "homepage": "https://oromedialab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OroMediaLab\\NxCoreBundle\\": "src/",
            "OroMediaLab\\NxCoreBundle\\Config\\": "config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OroMediaLab\\NxCoreBundle\\Tests\\": "tests/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "repositories": [
        {
            "url": "https://github.com/oromedialab/DoctrineBehaviors.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^7.0.4",
        "symfony/orm-pack": "^2.4.1",
        "symfony/security-bundle": "^7.0.5",
        "knplabs/doctrine-behaviors": "2.6.3",
        "symfony/yaml": "^7.0.3",
        "symfony/dotenv": "^7.0.4",
        "lexik/jwt-authentication-bundle": "^2.20.3",
        "aws/aws-sdk-php": "^3.300",
        "symfony/mime": "^7.0.3",
        "symfony/google-mailer": "^7.0.3",
        "symfony/mailer": "^7.0.4",
        "phpoffice/phpspreadsheet": "dev-master"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.0.4",
        "dama/doctrine-test-bundle": "^8.0.2"
    }
}
